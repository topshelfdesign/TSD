}
#custom-pager a:hover {
	color: #ef853c;
	border: 1px solid #ef853c;
	cursor: pointer;
	box-shadow: 0px 0px 2px #ef853c;
}


#custom-pager a {
	height: 60px;
	margin-bottom: 0;
	font-size: .75rem;
	line-height: 1.35rem;
	font-weight: 800;
	font-family: "jaf-domus-titling-web", sans-serif;
	border-bottom: none;
}

#custom-pager .cycle-pager-active a {
	border-bottom: none;
}
#custom-pager .cycle-pager-active a:after  {
	content: '';
	width: 75px;
	height: 75px;
	background: #fff;
	display: inline-block;
	border-radius: 15px;
	transform:rotate(45deg);
	-ms-transform:rotate(45deg); /* IE 9 */
	-webkit-transform:rotate(45deg); /* Opera, Chrome, and Safari */
	margin-top: 5px;
	box-sizing: border-box;
	position: absolute;
	margin-left: -41px;
	z-index: -1;
}
#custom-pager .cycle-pager-active a:hover:after {
/*	margin-top: -16px;
	margin-left: -37px;*/
}
#custom-pager .cycle-pager-active a:hover {
	box-shadow: none;
	border: none;
}

