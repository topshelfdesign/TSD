	margin: 1rem 0 1rem -2.375rem;
	width: 100%;
	min-width: 115%;
}
#blog-content h2 {
	line-height: 2.25rem;
	margin-bottom: 25px;
}
#blog-content h4 {
	margin-bottom: 1.25rem;
}
#blog-content p:last-of-type {
	margin-bottom: 0;
}

#blog-content .button.more {
	border-radius: 0 0 20px 20px ;
}

#blog-content .text-blog img.lead-in {
	margin-top: -2.375rem;
}
#blog-content .tags h6, #blog-content .tags h6 .title,
.single-post .tags h6, .single-post .tags h6 .title {
	color: #666;
	padding-bottom: 3px;
}
#blog-content .tags h6 .title:hover,
.single-post .tags h6 .title:hover {
	background: transparent;
	cursor: default;
}
.blog .pagination-centered {
	margin-bottom: 50px;
}
.blog .pagination-centered a {
	border-bottom: none;
	border-radius: 7px;
}
.blog .pagination-centered li.current a {
	background: #ef853c;
}
.blog .pagination-centered li.current a:hover {
	background: #ef853c;
}
.blog .arrow.icon {
	line-height: 1.75rem;
	font-size: 1.5rem;
}
.blog .sharing {
	border-top: 1px solid #efefef;
	padding-top: 5px;
	margin-bottom: 10px;
}
.blog .sharing img {
	border: none;
}

