/* Isotope */

/* clear fix */
.isotope:after {
  content: '';
  display: block;
  clear: both;
}

/* Small  */
@media only screen and (max-width: 40em) {
	.grid-sizer {
		width: 50%;
	}
}
/* Medium */
@media only screen and (min-width:40.063em) {
	.grid-sizer {
		width: 33.33333%;
	}
	
	#main {
		padding-bottom: 195px;
	}

    .interior #menu #menu-interior-menu > li a {
        padding-right: 8px !important;
    }

    .interior #menu #menu-interior-menu > li {
        padding-top: 0 !important;
        margin-top: 10px !important;
    }

    .interior .top-bar-section #menu-interior-menu ul.dropdown {
        top: 28px !important;
    }

}
/* Large */
@media only screen and (min-width:64.063em) {
	.grid-sizer {
		width: 25%;
	}

    .interior #menu #menu-interior-menu > li a {
        padding-right: 15px !important;
    }
}
/* XLarge */
@media only screen and (min-width: 90.063em) {
	.grid-sizer {
		width: 25%
	}


    .interior #menu #menu-interior-menu > li a {
        padding-right: 15px !important;
    }
}
/* XXLarge */
@media only screen and (min-width: 120.063em) {
	.grid-sizer {
		width: 16.66667%
	}


    .interior #menu #menu-interior-menu > li a {
        padding-right: 15px !important;
    }
}

